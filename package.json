    "@types/react": {
      "optional": true
    },
    "use-sync-external-store": {
      "optional": true
    },
    "@types/use-sync-external-store": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@types/node": "^20.5.7",
    "publint": "^0.2.8",
    "react": "19.0.0-rc-e684ca66ab-20240619",
